{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix --no-cache",
    "format": "prettier --write . --ignore-path .gitignore",
    "unused": "yarn ts-unused-exports tsconfig.json --excludeDeclarationFiles --showLineNumber '--excludePathsFromReport=pages' '--ignoreFiles=.*config.*|.*.stories.*'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes --only-changed",
    "cypress": "cypress open",
    "cy:test": "kill $(lsof -i :3009 | grep node | awk '{print $2}') & next build && next start --port 3009 & cypress run --browser chrome && kill $(lsof -i :3009 | grep node | awk '{print $2}') ",
    "prepare": "husky install",
    "upgrade:linter": "yarn up 'eslint*'",
    "upgrade:types": "yarn up '@types/*' '*typescript*'",
    "upgrade:storybook": "npx storybook@next upgrade --prerelease"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "printWidth": 100
  },
  "dependencies": {
    "@formspree/react": "^2.5.0",
    "@headlessui/react": "^1.7.16",
    "@notionhq/client": "^2.2.11",
    "@radix-ui/react-label": "^2.0.1",
    "@radix-ui/react-separator": "^1.0.2",
    "@vercel/analytics": "^1.0.2",
    "framer-motion": "^10.16.0",
    "i18next": "^22.4.12",
    "jotai": "^2.3.1",
    "next": "^13.5.2",
    "next-i18next": "^13.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.10.1",
    "react-textarea-autosize": "^8.5.3",
    "styled-components": "^6.0.7",
    "swr": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-syntax-typescript": "^7.20.0",
    "@playwright/test": "^1.37.1",
    "@storybook/addon-a11y": "^7.4.0-alpha.0",
    "@storybook/addon-actions": "^7.4.0-alpha.0",
    "@storybook/addon-essentials": "^7.4.0-alpha.0",
    "@storybook/addon-interactions": "^7.4.0-alpha.0",
    "@storybook/addon-links": "^7.4.0-alpha.0",
    "@storybook/addon-mdx-gfm": "^7.4.0-alpha.0",
    "@storybook/addon-styling": "^2.0.2-next.3",
    "@storybook/jest": "^0.1.1-next.2",
    "@storybook/nextjs": "7.4.0-alpha.0",
    "@storybook/react": "^7.4.0-alpha.0",
    "@storybook/testing-library": "^0.2.1-next.0",
    "@testing-library/react": "^14.0.0",
    "@types/gtag.js": "^0.0.13",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "chromatic": "^7.1.0",
    "cypress": "^13.2.0",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.16",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-css": "^0.8.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.28",
    "prettier": "^3.0.3",
    "stailwc": "^0.17.0",
    "storybook": "^7.4.0-alpha.0",
    "storybook-addon-swc": "^1.1.9",
    "tailwindcss": "^3.3.3",
    "ts-prune": "^0.10.3",
    "ts-unused-exports": "^10.0.1",
    "twin.macro": "^3.4.0",
    "typescript": "^5.1.6"
  },
  "volta": {
    "node": "18.14.2",
    "yarn": "3.6.0"
  }
}
