{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix --no-cache",
    "format": "prettier --write . --ignore-path .gitignore",
    "unused": "yarn ts-unused-exports tsconfig.json --excludeDeclarationFiles --showLineNumber '--excludePathsFromReport=pages' '--ignoreFiles=.*config.*|.*.stories.*'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes --only-changed",
    "cypress": "cypress open",
    "cy:test": "kill $(lsof -i :3009 | grep node | awk '{print $2}') & next build && next start --port 3009 & cypress run --browser chrome && kill $(lsof -i :3009 | grep node | awk '{print $2}') ",
    "prepare": "husky install",
    "upgrade:linter": "yarn up 'eslint*'",
    "upgrade:types": "yarn up '@types/*' '*typescript*'",
    "upgrade:storybook": "npx storybook@next upgrade --prerelease"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "printWidth": 100
  },
  "dependencies": {
    "@formspree/react": "^2.4.1",
    "@headlessui/react": "^1.7.13",
    "@notionhq/client": "^2.2.3",
    "@radix-ui/react-label": "^2.0.1",
    "@radix-ui/react-separator": "^1.0.2",
    "@vercel/analytics": "^0.1.11",
    "framer-motion": "^10.12.12",
    "i18next": "^22.4.12",
    "jotai": "^2.0.3",
    "next": "^13.2.4",
    "next-i18next": "^13.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.8.0",
    "react-textarea-autosize": "^8.4.0",
    "styled-components": "^5.3.9",
    "swr": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-syntax-typescript": "^7.20.0",
    "@playwright/test": "^1.31.2",
    "@storybook/addon-a11y": "^7.0.0-rc.8",
    "@storybook/addon-actions": "^7.0.0-rc.8",
    "@storybook/addon-essentials": "^7.0.0-rc.8",
    "@storybook/addon-interactions": "^7.0.0-rc.8",
    "@storybook/addon-links": "^7.0.0-rc.8",
    "@storybook/addon-mdx-gfm": "^7.0.0-rc.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/jest": "^0.0.11-next.1",
    "@storybook/nextjs": "7.0.0-rc.8",
    "@storybook/react": "^7.0.0-rc.8",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@testing-library/react": "^14.0.0",
    "@types/gtag.js": "^0.0.12",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.54.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "chromatic": "^6.17.2",
    "cypress": "^12.8.1",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-css": "^0.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "stailwc": "^0.15.0",
    "storybook": "^7.0.0-rc.8",
    "storybook-addon-swc": "^1.1.9",
    "tailwindcss": "^3.2.7",
    "ts-prune": "^0.10.3",
    "ts-unused-exports": "^9.0.4",
    "twin.macro": "^3.1.0",
    "typescript": "^5.0.2"
  },
  "volta": {
    "node": "18.14.2",
    "yarn": "1.22.17"
  }
}
